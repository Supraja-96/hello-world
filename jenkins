pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh "mvn package"
            }
        }
        stage('Test') {
            steps {
                sh "mvn test"
            }
        }
        stage('Deploy') {
            steps {
                http://3.14.255.230:8080/tomcat/tomcat/
            }
        }
    }
}
